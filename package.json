{
  "name": "n8n-nodes-procore",
  "version": "1.0.0",
  "description": "A comprehensive n8n community node for Procore construction management platform providing 20 resources and 100+ operations for complete project lifecycle management.",
  "license": "BUSL-1.1",
  "author": {
    "name": "Velocity BPA",
    "email": "licensing@velobpa.com",
    "url": "https://velobpa.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Velocity-BPA/n8n-nodes-procore.git"
  },
  "homepage": "https://github.com/Velocity-BPA/n8n-nodes-procore#readme",
  "bugs": {
    "url": "https://github.com/Velocity-BPA/n8n-nodes-procore/issues"
  },
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "procore",
    "construction",
    "project-management",
    "construction-management",
    "rfi",
    "submittals",
    "punch-list",
    "daily-logs",
    "contracts",
    "change-orders",
    "invoices",
    "document-management",
    "workflow-automation"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/ProcoreOAuth2Api.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Procore/Procore.node.js",
      "dist/nodes/Procore/ProcoreTrigger.node.js"
    ]
  },
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.ts\"",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "prepublishOnly": "npm run build",
    "clean": "rimraf dist"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "gulp-rename": "^2.0.0",
    "jest": "^29.7.0",
    "n8n-core": "^1.27.0",
    "n8n-workflow": "^1.27.0",
    "prettier": "^3.2.4",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "n8n-workflow": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
